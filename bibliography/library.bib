pages = {ra53},
pmid = {21868359},
title = {{A polymorphism-specific "memory" mechanism in the $\beta$(2)-adrenergic receptor.}},
url = {http://stke.sciencemag.org/cgi/doi/10.1126/scisignal.2001681 http://www.ncbi.nlm.nih.gov/pubmed/21868359},
volume = {4},
year = {2011}
}
