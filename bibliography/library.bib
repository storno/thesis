@article{Pera2014,
abstract = {The M3 and M2 muscarinic acetylcholine receptors (mAChRs) and beta-2-adrenoceptors ($\beta$2ARs) are important regulators of airway cell function, and drugs targeting these receptors are among the first line drugs in the treatment of the obstructive lung diseases asthma and chronic obstructive lung disease (COPD). Cross-regulation or crosstalk between mAChRs and $\beta$2ARs in airway smooth muscle (ASM) helps determine the contractile state of the muscle, thus airway diameter and resistance to airflow. In this review we will detail mAChR and $\beta$2AR-signaling and crosstalk, focusing on events in the ASM cell but also addressing the function of these receptors in other cell types that impact airway physiology. We conclude by discussing how recent advances in GPCR pharmacology offer a unique opportunity to fine tune mAChR and $\beta$2AR signaling and their crosstalk, and thereby produce superior therapeutics for obstructive lung and other diseases.},
author = {Pera, Tonio and Penn, Raymond B},
doi = {10.1016/j.coph.2014.03.005},
file = {:Users/ska/Documents/thesis/literature/2014/Pera, Penn/Pera, Penn - 2014 - Crosstalk between beta-2-adrenoceptor and muscarinic acetylcholine receptors in the airway.pdf:pdf},
issn = {1471-4973},
journal = {Curr Opin Pharmacol},
pages = {72--81},
