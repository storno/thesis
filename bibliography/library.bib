issn = {1526-632X},
journal = {Neurology},
keywords = {Animals,Cell Line,Corpus Striatum,Corpus Striatum: metabolism,Dimerization,Humans,Macromolecular Substances,Mice,Parkinson Disease,Parkinson Disease: metabolism,Parkinson Disease: therapy,Receptor, Adenosine A2A,Receptor, Adenosine A2A: metabolism,Receptor, Metabotropic Glutamate 5,Receptors, Dopamine D2,Receptors, Dopamine D2: metabolism,Receptors, Metabotropic Glutamate,Receptors, Metabotropic Glutamate: metabolism,Signal Transduction,Signal Transduction: physiology,gamma-Aminobutyric Acid,gamma-Aminobutyric Acid: metabolism},
